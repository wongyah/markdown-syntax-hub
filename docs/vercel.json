{
  "trailingSlash": true,
  "rewrites": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "markdown-syntax-hub.tcblabber.cn"
        }
      ],
      "destination": "/markdown-syntax-hub/"
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "markdown-syntax-hub.tcblabber.cn"
        }
      ],
      "destination": "/markdown-syntax-hub/:path*"
    }
  ]
}
