{
  "rewrites": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "markdown-syntax-hub.tcblabber.cn"
        }
      ],
      "destination": "/markdown-syntax-hub/:path*"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://www.tcblabber.cn/markdown-syntax-hub"
    }
  ]  
}